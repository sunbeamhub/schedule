{
  "name": "schedule",
  "version": "0.0.2",
  "private": true,
  "homepage": "https://sunbeamhub.github.io/schedule",
  "dependencies": {
    "@microsoft/fast-components": "^2.19.1",
    "@microsoft/fast-element": "^1.7.0",
    "@microsoft/fast-foundation": "^2.32.1",
    "@microsoft/fast-react-wrapper": "^0.1.24",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "env-cmd": "^10.1.0",
    "gh-pages": "^3.2.3",
    "graphql": "^16.2.0",
    "graphql.macro": "^1.4.2",
    "http-proxy-middleware": "^2.0.2",
    "husky": "^7.0.4",
    "jest-watch-typeahead": "^0.6.5",
    "lint-staged": "^12.3.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "roving-ux-react": "^1.2.3",
    "sass": "^1.49.0",
    "source-map-explorer": "^2.5.2",
    "swr": "^1.2.0",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "predeploy": "npm run build:github",
    "deploy": "gh-pages -d build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:github": "env-cmd -f .env.github npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-expressions": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/node-logger": "^6.4.14",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.14",
    "prettier-plugin-organize-imports": "^2.3.4",
    "webpack": "^5.67.0"
  }
}
